{
  "manifest_version": 3,
  "name": "ScratchProjectConnect",
  "version": "1.0.0",
  "description": "",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://scratch.mit.edu/projects/*"
      ],
      "js": [
        "project.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "modules/*"
      ],
      "matches": [
        "*://scratch.mit.edu/projects/*"
      ]
    }
  ],
  "project_modules": [
    "modules/modpow.js"
  ]
}